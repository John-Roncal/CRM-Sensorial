# .env - copia esto a .env y completa valores
# DSN/Connection string para SQL Server (ajusta driver/host/user/password)
DB_DSN=DRIVER={ODBC Driver 17 for SQL Server};SERVER=localhost;DATABASE=DBCRMSensorialCentral;Trusted_Connection=Yes;TrustServerCertificate=Yes;
# Token compartido entre servicios (debe coincidir con appsettings.ServiceToken en ASP.NET)
SERVICE_TOKEN=your_service_token_here

# Cohere API key (reemplaza con tu key)
COHERE_API_KEY=your_cohere_api_key_here

# Puerto local a usar
PORT=8000
